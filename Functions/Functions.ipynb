    "import heapq\n",
    "from operator import itemgetter\n",
    "f = open(\"retail.txt\",\"r\")\n",
    "items_freq = {}\n",
    "item_pairs_freq = {}\n",
    "\n",
    "#Top 10 items\n",
    "for line in f:\n",
    "    line = line.split()\n",
    "    line.pop(0)\n",
    "    if \"0\" in line:\n",
    "        line.remove(\"0\")\n",
    "       \n",
    "    for i in range(0, len(line)):\n",
    "        line[i] = int(line[i])\n",
    "   \n",
    "    line.sort()\n",
    "   \n",
    "    for item in line:\n",
    "        if item in items_freq:\n",
    "            items_freq[item] += 1\n",
    "        else:\n",
    "            items_freq[item] = 1\n",
    "\n",
    "#     Top 10 item pairs\n",
    "\n",
    "    for i in range(0, len(line)):\n",
    "        line[i] = int(line[i])\n",
    "   \n",
    "    line.sort()\n",
    "   \n",
    "    if len(line) == 2:\n",
    "        line = tuple(line)\n",
    "        if line in item_pairs_freq:\n",
    "                item_pairs_freq[line] += 1\n",
    "        else:\n",
    "                item_pairs_freq[line] = 1\n",
    "    else:\n",
    "         continue\n",
    "\n",
    "#https://howtodoinjava.com/python/examples/find-largest-smallest-items/       \n",
    "topitems = dict(heapq.nlargest(10, items_freq.items(), key=itemgetter(1)))\n",
    "topitempairs = dict(heapq.nlargest(10, item_pairs_freq.items(), key=itemgetter(1)))\n",
    "print(topitems)\n",
    "print(topitempairs)\n",
    "\n",
    "\n",
    "new_file = open(\"output.txt\", \"w+\")\n",
    "\n",
    "#Print top 10 items\n",
    "new_file.write(\"Top 10 items:\\n\")\n",
    "for i in topitems:\n",
    "        new_file.write(\"%i\\n\" % i)\n",
    "\n",
    "#Print top 10 item pairs\n",
    "new_file.write(\"Top 10 item pairs:\\n\")\n",
    "for i in topitempairs.keys():\n",
    "    line = ', '.join(str(n) for n in i)\n",
    "    new_file.write(line + '\\n')\n",
    "new_file.close()"
   ]
