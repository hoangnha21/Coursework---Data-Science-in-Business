    "#Exercise 1\n",
    "import math \n",
    "num= input(\"\")\n",
    "ogx =0\n",
    "ogy =0\n",
    "\n",
    "x=0\n",
    "y=0\n",
    "\n",
    "while num!=\"X\":\n",
    "    num=num.split(' ')\n",
    "    \n",
    "    if num[0]==\"UP\":\n",
    "        y=y+int(num[1])\n",
    "    elif num[0]== \"RIGHT\":\n",
    "        x=x+int(num[1])\n",
    "    elif num[0]== \"DOWN\":\n",
    "        y=y-int(num[1])\n",
    "    elif num[0] == \"LEFT\":\n",
    "        x=x-int(num[1])\n",
    "        \n",
    "    num= input(\" \")\n",
    "\n",
    "distance = abs((x-ogx))+abs((y-ogy))\n",
    "\n",
    "print(distance)"
