    "#Exercise 2\n",
    "import string\n",
    "#Create a list to store all sublists of words from different articles\n",
    "cleaned_ar = []\n",
    "\n",
    "#Create a loop adding articles until user clicks X\n",
    "while True:\n",
    "   \n",
    "    #Asking user for another article until they click X\n",
    "    ar = input()\n",
    "    if ar != \"X\":\n",
    "        #Remove punctuation\n",
    "        #https://stackoverflow.com/questions/265960/best-way-to-strip-punctuation-from-a-string\n",
    "        ar = ar.translate({ord(c): None for c in string.punctuation})\n",
    "        #Turn everything to lowercase\n",
    "        ar = ar.lower()\n",
    "        #Seperate words by space\n",
    "        ar = ar.split()\n",
    "        #Sort alphabetically\n",
    "        ar = sorted(ar)\n",
    "       \n",
    "       \n",
    "        cleaned_ar.extend(ar)\n",
    "        \n",
    "        freq = {}\n",
    "        for word in cleaned_ar:\n",
    "            if word in freq:\n",
    "                freq[word] += 1\n",
    "            else:\n",
    "                freq[word] = 1\n",
    "   \n",
    "    else:\n",
    "        break\n",
    "        \n",
    "for k,v in freq.items():\n",
    "    print(\"{}: {}\".format(k,v))\n"
   ]
